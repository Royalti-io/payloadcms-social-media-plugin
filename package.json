{
  "name": "@payloadcms/plugin-social-media",
  "version": "1.0.0",
  "description": "Social media integration plugin for PayloadCMS 3.0+",
  "keywords": [
    "payload",
    "payloadcms",
    "plugin",
    "social-media",
    "social",
    "media",
    "cms"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "author": "PayloadCMS",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Royalti-io/payloadcms-social-media-plugin.git"
  },
  "bugs": {
    "url": "https://github.com/Royalti-io/payloadcms-social-media-plugin/issues"
  },
  "homepage": "https://payloadcms.com",
  "peerDependencies": {
    "payload": ">=3.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "@types/react": "^19.1.12",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "payload": ">=3.0.0",
    "prettier": "^3.2.4",
    "react": "^19.1.1",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
